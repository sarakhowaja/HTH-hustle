        "import turtle\n",
        "claire = turtle.Turtle()\n",
        "claire.shape('turtle')\n",
        "\n",
        "claire.penup()\n",
        "#Move turtle a little further\n",
        "claire.forward(50)\n",
        "#change the text?\n",
        "claire.write(\"Hello, I'm Claire!\")\n",
        "#move it more steps backward\n",
        "claire.backward(100)\n",
        "claire.write(\"Nice to meet you\")\n",
        "claire.backward(30)\n",
        "#strings are on top of eachother\n",
        "#fix: move turtle further back\n",
        "\n",
        "#https://hourofpython.trinket.io/a-visual-introduction-to-python#/turtles/meet-tina"
